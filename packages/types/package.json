{
  "name": "@signaltree/types",
  "version": "1.0.0",
  "description": "TypeScript type definitions for SignalTree.",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "README.md",
    "package.json",
    "dist"
  ],
  "keywords": [
    "signaltree",
    "types",
    "typescript"
  ],
  "author": "SignalTree",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@angular/core": "^20.3.0"
  }
}
