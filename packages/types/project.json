{
  "name": "types",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/types/src",
  "projectType": "library",
  "tags": ["npm:public", "scope:types"],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "main": "packages/types/src/index.ts",
        "tsConfig": "packages/types/tsconfig.lib.prod.json",
        "outputPath": "dist/packages/types",
        "format": ["esm"],
        "assets": [
          {
            "input": "packages/types",
            "glob": "README.md",
            "output": "."
          },
          {
            "input": "packages/types",
            "glob": "package.json",
            "output": "."
          },
          {
            "input": "packages/types/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          }
        ],
        "rollupConfig": "packages/types/rollup.config.mjs",
        "deleteOutputPath": true,
        "buildLibsFromSource": false,
        "useLegacyTypescriptPlugin": false,
        "generatePackageJson": false
      }
    }
  }
}
