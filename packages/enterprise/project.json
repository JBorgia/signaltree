{
  "name": "enterprise",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/enterprise/src",
  "projectType": "library",
  "tags": ["scope:enterprise", "type:feature"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/enterprise",
        "main": "packages/enterprise/src/index.ts",
        "tsConfig": "packages/enterprise/tsconfig.lib.json",
        "assets": ["packages/enterprise/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/enterprise/jest.config.ts"
      }
    }
  }
}
