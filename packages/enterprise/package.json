{
  "name": "@signaltree/enterprise",
  "version": "8.0.1",
  "description": "Enterprise optimizations for SignalTree reactive JSON. Diff-based updates, bulk operations, and advanced change tracking for large-scale applications.",
  "license": "BSL-1.1",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./diff-engine": {
      "types": "./src/lib/diff-engine.d.ts",
      "import": "./dist/lib/diff-engine.js",
      "default": "./dist/lib/diff-engine.js"
    },
    "./path-index": {
      "types": "./src/lib/path-index.d.ts",
      "import": "./dist/lib/path-index.js",
      "default": "./dist/lib/path-index.js"
    },
    "./update-engine": {
      "types": "./src/lib/update-engine.d.ts",
      "import": "./dist/lib/update-engine.js",
      "default": "./dist/lib/update-engine.js"
    },
    "./enterprise-enhancer": {
      "types": "./src/lib/enterprise-enhancer.d.ts",
      "import": "./dist/lib/enterprise-enhancer.js",
      "default": "./dist/lib/enterprise-enhancer.js"
    },
    "./scheduler": {
      "types": "./src/lib/scheduler.d.ts",
      "import": "./dist/lib/scheduler.js",
      "default": "./dist/lib/scheduler.js"
    },
    "./thread-pools": {
      "types": "./src/lib/thread-pools.d.ts",
      "import": "./dist/lib/thread-pools.js",
      "default": "./dist/lib/thread-pools.js"
    }
  },
  "keywords": [
    "angular",
    "signals",
    "state-management",
    "enterprise",
    "performance",
    "optimization",
    "bulk-updates",
    "diff",
    "monitoring"
  ],
  "author": "Jonathan D Borgia",
  "homepage": "https://github.com/JBorgia/signaltree#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/JBorgia/signaltree.git",
    "directory": "packages/enterprise"
  },
  "bugs": {
    "url": "https://github.com/JBorgia/signaltree/issues"
  },
  "peerDependencies": {
    "@angular/core": "^20.0.0",
    "@signaltree/core": "workspace:*",
    "tslib": "^2.0.0"
  },
  "peerDependenciesMeta": {},
  "devDependencies": {
    "@signaltree/core": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "src/**/*.d.ts",
    "README.md"
  ]
}
