{
  "name": "callable-syntax",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/callable-syntax/src",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": ["dist/{projectRoot}"],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": ["scope:syntax", "type:tooling"],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "dependsOn": ["^build"],
      "options": {
        "main": "packages/callable-syntax/src/index.ts",
        "tsConfig": "packages/callable-syntax/tsconfig.lib.prod.json",
        "outputPath": "dist/packages/callable-syntax",
        "format": ["esm"],
        "additionalEntryPoints": [
          "packages/callable-syntax/src/index.ts",
          "packages/callable-syntax/src/lib/vite-plugin.ts",
          "packages/callable-syntax/src/lib/webpack-plugin.ts",
          "packages/callable-syntax/src/augmentation.ts"
        ],
        "assets": [
          {
            "input": "packages/callable-syntax",
            "glob": "README.md",
            "output": "."
          },
          {
            "input": "packages/callable-syntax",
            "glob": "package.json",
            "output": "."
          },
          {
            "input": "packages/callable-syntax",
            "glob": "augmentation.d.ts",
            "output": "."
          }
        ],
        "rollupConfig": "packages/callable-syntax/rollup.config.mjs",
        "deleteOutputPath": true,
        "buildLibsFromSource": false,
        "useLegacyTypescriptPlugin": false,
        "generatePackageJson": false
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/callable-syntax/jest.config.ts"
      }
    }
  }
}
