{
  "name": "@signaltree/core",
  "version": "1.1.7",
  "description": "Lightweight, framework-adaptable signal-based state management. Core primitives only (Angular adapter currently internal).",
  "type": "module",
  "peerDependencies": {
    "@angular/core": "^20.1.0",
    "lodash-es": "^4.17.21"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./fesm2022/signaltree-core.mjs"
    },
    "./vanilla": {
      "types": "./vanilla/index.d.ts",
      "import": "./fesm2022/signaltree-core.mjs"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "license": "MIT",
  "author": "Jonathan D Borgia",
  "repository": {
    "type": "git",
    "url": "https://github.com/JBorgia/signaltree"
  },
  "homepage": "https://github.com/JBorgia/signaltree#readme",
  "bugs": {
    "url": "https://github.com/JBorgia/signaltree/issues"
  },
  "keywords": [
    "signals",
    "state",
    "state-management",
    "angular",
    "signaltree",
    "store"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "esm2022/**/*",
    "index.d.ts",
    "README.md",
    "LICENSE",
    "package.json"
  ]
}
