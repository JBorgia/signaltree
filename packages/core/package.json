{
  "name": "@signaltree/core",
  "version": "4.0.9",
  "description": "Lightweight, type-safe signal-based state management for Angular. Core package providing hierarchical signal trees, basic entity management, and async actions.",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./enhancers": {
      "import": "./dist/enhancers/index.js",
      "types": "./dist/enhancers/index.d.ts"
    },
    "./enhancers/batching": {
      "import": "./dist/enhancers/batching/index.js",
      "types": "./dist/enhancers/batching/index.d.ts"
    },
    "./enhancers/computed": {
      "import": "./dist/enhancers/computed/index.js",
      "types": "./dist/enhancers/computed/index.d.ts"
    },
    "./enhancers/middleware": {
      "import": "./dist/enhancers/middleware/index.js",
      "types": "./dist/enhancers/middleware/index.d.ts"
    },
    "./enhancers/memoization": {
      "import": "./dist/enhancers/memoization/index.js",
      "types": "./dist/enhancers/memoization/index.d.ts"
    },
    "./enhancers/time-travel": {
      "import": "./dist/enhancers/time-travel/index.js",
      "types": "./dist/enhancers/time-travel/index.d.ts"
    },
    "./enhancers/devtools": {
      "import": "./dist/enhancers/devtools/index.js",
      "types": "./dist/enhancers/devtools/index.d.ts"
    },
    "./enhancers/presets": {
      "import": "./dist/enhancers/presets/index.js",
      "types": "./dist/enhancers/presets/index.d.ts"
    },
    "./enhancers/entities": {
      "import": "./dist/enhancers/entities/index.js",
      "types": "./dist/enhancers/entities/index.d.ts"
    },
    "./enhancers/serialization": {
      "import": "./dist/enhancers/serialization/index.js",
      "types": "./dist/enhancers/serialization/index.d.ts"
    }
  },
  "peerDependencies": {
    "@angular/core": "^20.3.0",
    "jest-preset-angular": "*",
    "@rollup/plugin-commonjs": "*",
    "@rollup/plugin-node-resolve": "*",
    "@rollup/plugin-typescript": "*",
    "rollup": "*",
    "rollup-plugin-dts": "*",
    "tslib": "*"
  },
  "dependencies": {
    "@signaltree/shared": "workspace:*"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ]
}
